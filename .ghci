:set prompt "% "
:set -XOverloadedStrings
:m + Data.Text
:m + Data.Attoparsec.Text
:m + Language.Lambad.Eval
:m + Language.Lambad.Misc
:m + Language.Lambad.Syntax
:m + Language.Lambad.Parser
:m + Language.Lambad.Pretty

let eval v e = putStrLn (either unpack renderString $ runEval defaultEnv =<< v `fmap` (lmap pack $ parseOnly parseExpr e))
